{
  "name": "react-feedback-report-widget",
  "version": "1.0.0",
  "description": "A typesafe React feedback widget with shake detection and customizable UI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "publishConfig": { 
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "release": "semantic-release",
    "publish:npm": "npm publish --access=public"
  },
  "keywords": [
    "feedback",
    "react",
    "typescript",
    "widget",
    "shake-detection",
    "bug-report",
    "user-feedback",
    "feedback-widget",
    "react-component"
  ],
  "author": {
    "name": "ArlindMaliqi",
    "url": "https://github.com/ArlindMaliqi"
  },
  "license": "MIT",
  "dependencies": {
    "shake.js": "^1.2.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "rimraf": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "semantic-release": "^22.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^9.0.0",
    "@semantic-release/npm": "^11.0.0",
    "@semantic-release/release-notes-generator": "^12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArlindMaliqi/feedback-report.git"
  },
  "bugs": {
    "url": "https://github.com/ArlindMaliqi/feedback-report/issues"
  },
  "homepage": "https://github.com/ArlindMaliqi/feedback-report#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}