{
  "name": "react-feedback-report-widget",
  "version": "2.0.0",
  "description": "A comprehensive TypeScript-first React feedback widget with shake detection, powerful integrations, analytics, and professional developer experience",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "sideEffects": false,
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "release": "semantic-release",
    "publish:npm": "npm publish --access=public",
    "pack:dist": "tar -czf dist.tar.gz -C dist .",
    "prepublishOnly": "npm run build",
    "publish:github": "npm publish --registry=https://npm.pkg.github.com",
    "build:github": "npm run build && npm run package:github",
    "package:github": "node -e \"const pkg = require('./package.json'); pkg.name = '@ArlindMaliqi/react-feedback-report-widget'; pkg.publishConfig.registry = 'https://npm.pkg.github.com'; require('fs').writeFileSync('package.github.json', JSON.stringify(pkg, null, 2));\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "npm run build && npx bundlesize",
    "analyze:webpack": "npm run build && npx webpack-bundle-analyzer dist/stats.json",
    "perf:audit": "npm run build && npx lighthouse-ci autorun",
    "size-limit": "npx size-limit"
  },
  "keywords": [
    "react",
    "typescript",
    "feedback",
    "widget",
    "shake-detection",
    "bug-report",
    "user-feedback",
    "feedback-widget",
    "react-component",
    "analytics",
    "integrations",
    "accessibility",
    "a11y",
    "dark-mode",
    "light-mode",
    "theme",
    "offline-support",
    "pwa",
    "nextjs",
    "gatsby",
    "remix",
    "ssr",
    "performance",
    "bundle-optimization",
    "tree-shaking",
    "lazy-loading",
    "github-issues",
    "jira",
    "gitlab",
    "azure-devops",
    "slack",
    "teams",
    "discord",
    "webhooks",
    "google-analytics",
    "segment",
    "mixpanel",
    "i18n",
    "localization",
    "rtl",
    "testing",
    "storybook",
    "jest",
    "react-testing-library",
    "typescript-first",
    "enterprise",
    "production-ready",
    "mobile-friendly",
    "responsive",
    "wcag",
    "gdpr",
    "privacy",
    "security",
    "file-upload",
    "attachments",
    "voting",
    "categories",
    "customizable",
    "developer-experience",
    "dx"
  ],
  "author": {
    "name": "ArlindMaliqi",
    "url": "https://github.com/ArlindMaliqi"
  },
  "license": "MIT",
  "dependencies": {
    "shake.js": "^1.2.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-interactions": "^7.0.0",
    "@storybook/addon-links": "^7.0.0",
    "@storybook/blocks": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@storybook/react-webpack5": "^7.0.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "rimraf": "^5.0.0",
    "semantic-release": "^22.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^9.0.0",
    "@semantic-release/npm": "^11.0.0",
    "@semantic-release/release-notes-generator": "^12.0.0",
    "storybook": "^7.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArlindMaliqi/feedback-report.git"
  },
  "bugs": {
    "url": "https://github.com/ArlindMaliqi/feedback-report/issues"
  },
  "homepage": "https://github.com/ArlindMaliqi/feedback-report#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "bundlesize": [
    {
      "path": "./dist/index.js",
      "maxSize": "25 kB",
      "compression": "gzip"
    },
    {
      "path": "./dist/components/OptimizedFeedbackWidget.js",
      "maxSize": "15 kB",
      "compression": "gzip"
    }
  ],
  "size-limit": [
    {
      "name": "Core bundle",
      "path": "dist/index.js",
      "limit": "25 KB"
    },
    {
      "name": "Optimized widget",
      "path": "dist/components/OptimizedFeedbackWidget.js",
      "limit": "15 KB"
    }
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./testing": {
      "import": "./dist/testing/index.esm.js",
      "require": "./dist/testing/index.js",
      "types": "./dist/testing/index.d.ts"
    },
    "./storybook": {
      "import": "./dist/storybook/index.esm.js",
      "require": "./dist/storybook/index.js",
      "types": "./dist/storybook/index.d.ts"
    },
    "./examples": {
      "import": "./dist/examples/index.esm.js",
      "require": "./dist/examples/index.js",
      "types": "./dist/examples/index.d.ts"
    },
    "./optimized": {
      "import": "./dist/components/OptimizedFeedbackWidget.esm.js",
      "require": "./dist/components/OptimizedFeedbackWidget.js",
      "types": "./dist/components/OptimizedFeedbackWidget.d.ts"
    }
  }
}
