{
  "name": "react-feedback-report-widget",
  "version": "1.5.0",
  "description": "A typesafe React feedback widget with shake detection, integrations, and testing utilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "release": "semantic-release",
    "publish:npm": "npm publish --access=public",
    "pack:dist": "tar -czf dist.tar.gz -C dist .",
    "prepublishOnly": "npm run build",
    "publish:github": "npm publish --registry=https://npm.pkg.github.com",
    "build:github": "npm run build && npm run package:github",
    "package:github": "node -e \"const pkg = require('./package.json'); pkg.name = '@ArlindMaliqi/react-feedback-report-widget'; pkg.publishConfig.registry = 'https://npm.pkg.github.com'; require('fs').writeFileSync('package.github.json', JSON.stringify(pkg, null, 2));\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "feedback",
    "react",
    "typescript",
    "widget",
    "shake-detection",
    "bug-report",
    "user-feedback",
    "feedback-widget",
    "react-component",
    "analytics",
    "integrations",
    "nextjs",
    "gatsby",
    "remix"
  ],
  "author": {
    "name": "ArlindMaliqi",
    "url": "https://github.com/ArlindMaliqi"
  },
  "license": "MIT",
  "dependencies": {
    "shake.js": "^1.2.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-interactions": "^7.0.0",
    "@storybook/addon-links": "^7.0.0",
    "@storybook/blocks": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@storybook/react-webpack5": "^7.0.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "rimraf": "^5.0.0",
    "semantic-release": "^22.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^9.0.0",
    "@semantic-release/npm": "^11.0.0",
    "@semantic-release/release-notes-generator": "^12.0.0",
    "storybook": "^7.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArlindMaliqi/feedback-report.git"
  },
  "bugs": {
    "url": "https://github.com/ArlindMaliqi/feedback-report/issues"
  },
  "homepage": "https://github.com/ArlindMaliqi/feedback-report#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
